<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>weatherInfoView</welcome-file>
    </welcome-file-list>
<servlet>
    <servlet-name>WeatherInfoBackEndController</servlet-name>
    <servlet-class>com.eland.weatherInfo.controller.WeatherInfoBackEndController</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>WeatherInfoBackEndController</servlet-name>
    <url-pattern>/WeatherInfoBackEndController</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>weatherInfoView</servlet-name>
    <jsp-file>/view/weatherInfoView.jsp</jsp-file>
</servlet>
    <servlet-mapping>
        <servlet-name>weatherInfoView</servlet-name>
        <url-pattern>/weatherInfoView</url-pattern>
    </servlet-mapping>
</web-app>